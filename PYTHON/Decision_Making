# Input:
#     10
#     4
#     7
# Output:
#     10

num1=int(input())
num2=int(input())
num3=int(input())

if num1 >= num2 and num1 >= num3:
    print(num1)
elif num2 >= num3 and num2 >= num1:
    print(num2)
else:
    print(num3)

# Input:
#     32
#     11
# Output:
#     32

a = int(input())
b = int(input())

if a > b:
    print(a)
else:
    print(b)

# Input:
#     add
#     10 
#     3
# Output:
#     13

operators = input()
a =int(input())
b =int(input())

if operators == "add":
    print(a+b)
elif operators == "subtract":
    print(a-b)
elif operators == "multiply":
    print(a*b)
elif operators == "divide":
    if b==0:
        print("cannot be divided by zero")
    else:
        print(a/b)  
        
else:
    print("invalid input, Let's check once again...")

# Input:
#     4
#     5
# Output:
#     False

num1=int(input())
num2=int(input())
if num1==num2:
    print("True")
else:
    print("False")

# Input:
#     4
#     5
# Output:
