a = int(input())
b = int(input())
if a != b:
    print("True")
else:
    print("False")

# Input:
#     45
#     30
# Output:
#     Student passed!

score = int(input())
threshold = int(input())
if score > threshold:
    print("Student passed!")
else:
    print("Student failed!")

# Input:
#     55
# Output:
#     The number is less than 100

number=int(input())
if number < 100:
    print("The number is less than 100")
elif number == 100:
    print("The number is equal to 100")
else:
    print("The number is greater than 100")
    

# Input:
#     35
#     citizen
# Output:
#     You are eligible to vote.

age = int(input())
citizenship_status = input()

if age >= 18 and citizenship_status == "citizen":
    print("You are eligible to vote.")
else:
    print("You are not eligible to vote.")

# Input:
#     300
#     False
# Output:
#     You are eligible for free shipping.

totelOrderAmount =int(input())
premiumMember =input()

if totelOrderAmount >= 50 or premiumMember == "True":
    print("You are eligible for free shipping.")
else:
    print("You are not eligible for free shipping.")

# Input:
#     25
# Output:
